class Solution {
    public int countPartitions(int[] nums) {
        int n=nums.length;
        // int[] rightprefix=new int[n];
        int sum=0;
        for(int i=n-1;i>=0;i--){
            sum+=nums[i];
            // rightprefix[i]=sum;
        }
        // sum=0;
        int leftsum=0;
        int count=0;
        for(int i=0;i<n-1;i++){
            leftsum+=nums[i];
            int rightsum=sum-leftsum;
            if((leftsum%2==0 && rightsum%2==0) || (leftsum%2!=0 && rightsum%2!=0)) count++;
        }
        return count;
    }
}